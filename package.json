{
  "name": "ngx-signal-storage",
  "version": "0.1.1",
  "description": "Angular library for using local storage reactively with signals API",
  "scripts": {
    "ng": "ng",
    "watch:lib": "ng build ngx-signal-storage --watch",
    "build:lib": "ng build ngx-signal-storage",
    "test:lib": "jest --config projects/ngx-signal-storage/jest.config.ts",
    "start:sandbox": "ng serve",
    "publish": "ng build ngx-signal-storage && cd dist/ngx-signal-storage && npm publish && cd ../.."
  },
  "dependencies": {
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.5",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.4",
    "ng-packagr": "^17.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}